@typeparam T
@inherits MudBaseInput<T>

<MudSelect Class="@Class"
           Style="@Style"
           Tag="@Tag"
           UserAttributes="@UserAttributes"
           Disabled="@Disabled"
           ReadOnly="@ReadOnly"
           FullWidth="@FullWidth"
           Immediate="@Immediate"
           DisableUnderLine="@DisableUnderLine"
           HelperText="@HelperText"
           HelperTextOnFocus="@HelperTextOnFocus"
           AdornmentIcon="@AdornmentIcon"
           AdornmentText="@AdornmentText"
           Adornment="@Adornment"
           OnlyValidateIfDirty="@OnlyValidateIfDirty"
           AdornmentColor="@AdornmentColor"
           AdornmentAriaLabel="@AdornmentAriaLabel"
           IconSize="@IconSize"
           OnAdornmentClick="@OnAdornmentClick"
           Variant="@Variant"
           Margin="@Margin"
           Placeholder="@Placeholder"
           Counter="@Counter"
           MaxLength="@MaxLength"
           Label="@Label"
           AutoFocus="@AutoFocus"
           Lines="@Lines"
           Text="@Text"
           TextUpdateSuppression="@TextUpdateSuppression"
           InputMode="@InputMode"
           Pattern="@Pattern"
           TextChanged="@TextChanged"
           OnBlur="@OnBlur"
           OnInternalInputChanged="@OnInternalInputChanged"
           OnKeyDown="@OnKeyDown"
           KeyDownPreventDefault="@KeyDownPreventDefault"
           OnKeyUp="@OnKeyUp"
           KeyUpPreventDefault="@KeyUpPreventDefault"
           ValueChanged="@ValueChanged"
           Value="@Value"
           Format="@Format"
           OuterClass="@OuterClass"
           InputClass="@InputClass"
           OnOpen="@OnOpen"
           OnClose="@OnClose"
           PopoverClass="@PopoverClass"
           ListClass="@ListClass"
           Dense="@Dense"
           OpenIcon="@OpenIcon"
           CloseIcon="@CloseIcon"
           SelectAll="@SelectAll"
           SelectAllText="@SelectAllText"
           SelectedValuesChanged="@SelectedValuesChanged"
           MultiSelectionTextFunc="@MultiSelectionTextFunc"
           Delimiter="@Delimiter"
           SelectedValues="@SelectedValues"
           Comparer="@Comparer"
           ToStringFunc="@ToStringFunc">

    @foreach (var item in Items)
    {
        <MudSelectItem Value="@item">@item</MudSelectItem>
    }

</MudSelect>